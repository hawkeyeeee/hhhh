#include <iostream> // std::cout, std::endl

using namespace std;
int main()
{
  double x,y;
  char symbol;

  while (true)
  {
    std::cin >> x >> y >> symbol;
    // std::cout << (x > y ? 1 : 0) << std::endl;

    if (symbol == '+')
    {
      std::cout << x + y << std::endl;
    }
    elif (symbol == '*')
    {
      std::cout << x * y << std::endl;
    }
    elif (symbol == '-')
    {
      if (x>y);
      {
        std::cout << x - y << std::endl;
      }
      std::cout << y -x << std::endl;

    elif (symbol == '/')
    {
      std::cout << x / y << std::endl;
    }
    else
    {
      std::cout << "Error!" << std::endl;
    }
  }

  for (int i = 0; i < 10; ++i)
  {
    for (int j = 0; j < 10; ++j)
    {
    }
  }

  // while
  // do-while
  // for

  std::cout << "well done" << std::endl;
  system("pause");
  return 0;
}
